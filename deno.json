{
  "imports": {
    "@googleapis/androidpublisher": "npm:@googleapis/androidpublisher@^8.0.0",
    "google-auth-library": "npm:google-auth-library@^9.0.0",
    "@std/cli": "jsr:@std/cli@^1.0.22"
  },
  "tasks": {
    "run": "deno run --allow-read --allow-net --allow-env main.ts",
    "compile": "deno compile --output $OUTPUT_FILE_NAME --target $DENO_TARGET --allow-read --allow-net --allow-env main.ts",
    "compile-for-this-os": "deno compile --output upload-android-app-to-google-play --allow-read --allow-net --allow-env main.ts"
  }
}
